<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns1:ssm xmlns="ase5_SSM"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="ase5_SSM ssm_schema.xsd" xmlns:ns1="ase5_SSM">
    <ns1:Api base-url="http://192.168.1.214:8080"/>
    <ns1:SystemElement ns1:space_sys_obj_name="sut-robot" ns1:sys_elmt_absolute_name="sut-robot">
          <ns1:Activity  ns1:space_sys_obj_name="moveForward" ns1:act_descr="moveForward">
              <ns1:act_arg ns1:space_sys_obj_name="distance" ns1:act_arg_descr="distance" ns1:act_arg_dataType="Real"/>
          </ns1:Activity>
          <ns1:Activity ns1:space_sys_obj_name="moveBackwards" ns1:act_descr="moveBackwards">
              <ns1:act_arg ns1:space_sys_obj_name="distance" ns1:act_arg_descr="distance" ns1:act_arg_dataType="Real"/>
          </ns1:Activity>
          <ns1:Activity ns1:space_sys_obj_name="rotateClockwise" ns1:act_descr="rotateClockwise">
              <ns1:act_arg ns1:space_sys_obj_name="degrees" ns1:act_arg_descr="degrees" ns1:act_arg_dataType="SignedInteger"/>
          </ns1:Activity>
          <ns1:Activity ns1:space_sys_obj_name="rotateCounterclockwise" ns1:act_descr="rotateCounterclockwise">
              <ns1:act_arg ns1:space_sys_obj_name="degrees" ns1:act_arg_descr="degrees" ns1:act_arg_dataType="SignedInteger"/>
          </ns1:Activity>
          <ns1:Activity ns1:space_sys_obj_name="reset" ns1:act_descr="reset"/>
          <ns1:Activity ns1:space_sys_obj_name="stop" ns1:act_descr="stop"/>
          <ns1:ReportingData ns1:space_sys_obj_name="distanceFront" ns1:rd_descr="distanceFront" ns1:rd_dataType="Real"/>
          <ns1:ReportingData ns1:space_sys_obj_name="distanceDriven" ns1:rd_descr="distanceDriven" ns1:rd_dataType="Real"/>
          <ns1:ReportingData ns1:space_sys_obj_name="movementStatus" ns1:rd_descr="movementStatus" ns1:rd_dataType="Boolean"/>
          <ns1:ReportingData ns1:space_sys_obj_name="gyroscopeX" ns1:rd_descr="gyroscopeX" ns1:rd_dataType="Real"/>
          <ns1:ReportingData ns1:space_sys_obj_name="gyroscopeY" ns1:rd_descr="gyroscopeY" ns1:rd_dataType="Real"/>
          <ns1:ReportingData ns1:space_sys_obj_name="gyroscopeZ" ns1:rd_descr="gyroscopeZ" ns1:rd_dataType="Real"/>
          <ns1:Event ns1:space_sys_obj_name="noAnswer" ns1:event_descr="The connection to the SUT has been lost."/>
          <ns1:Event ns1:space_sys_obj_name="obstacleFound" ns1:event_descr="An obstacle has been detected on the defined path."/>
          <ns1:Event ns1:space_sys_obj_name="noPathLimit" ns1:event_descr="The location of the robot can't be defined. No objects in sight."/>
    </ns1:SystemElement>
</ns1:ssm>